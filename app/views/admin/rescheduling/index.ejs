     //CONVERSION FUNCTION
     function convertDate(cDate, splitChar) {

      if (splitChar == '/') {
        //31/08/2021
        let arr = cDate.split('/');
        return `${arr[2]}-${arr[1]}-${arr[0]}`
      } else if (splitChar == '-') {
        //2021-11-06
        let arr = cDate.split('-');
        return `${arr[2]}/${arr[1]}/${arr[0]}`
      }
    }

